subroutine obedot(neq, t, y, dydt, delta, dummy)
	use settings
	implicit none
	integer, intent(in) :: neq, dummy
	double precision, intent(in) :: t
	double complex, intent(in) :: y(neq)
	double complex, intent(out) :: dydt(neq)
	double precision, intent(in) :: delta(2)
	double precision :: dp, dc
	complex, parameter :: j = cmplx(0, 1)
	double precision, parameter :: uB = 1.4d0
	double precision, parameter :: root2 = dsqrt(2.d0)
	double precision, parameter :: root3 = dsqrt(3.d0)
	double precision, parameter :: root5 = dsqrt(5.d0)
	double precision, parameter :: root6 = dsqrt(6.d0)
	double precision, parameter :: root10 = dsqrt(10.d0)
	double precision, parameter :: root15 = dsqrt(15.d0)
	double precision, parameter :: root30 = dsqrt(30.d0)
	dp = delta(1)
	dc = delta(2)
	
end subroutine
